  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "electron": "^13.1.6",
    "electron-is-dev": "^2.0.0",
    "wait-on": "^6.0.0",
    "workbox-build": "^6.1.5",
    "express": "^4.17.1",
    "cors": "^2.8.5"
  }
